<!doctype html>
<html lang="en">
    <head>
        <meta charset="UTF-8">
        <title><%= projectName %></title>
        <style><%- codeStyle %><%- tmplStyle %><%- rootStyle %><%- processedCSS %></style>
    </head>
    <body>
        <header class="docs-header">
            <div class="container">
                <h1><%= projectName %></h1>
            </div>
        </header>
        <% maps.forEach(function (map) { %>
        <section>
            <div class="container">
                <div><%- map.html %></div>
                <div>
                    <pre class="code"><code class="lang-css"><%- map.rule %></code></pre>
                </div>
            </div>
        </section>
        <% })%>
        <footer>
            <div class="container">
                <p>Generated by <a href="https://github.com/morishitter/postcss-style-guide">postcss-style-guide</a></p>
            </div>
        </footer>
    </body>
</html>
